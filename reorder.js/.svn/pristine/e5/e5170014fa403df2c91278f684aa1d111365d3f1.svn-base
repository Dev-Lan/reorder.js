<!DOCTYPE html>
<meta charset="utf-8">
<title>Dijkstra Test</title>
<script src="../lib/science.v1.js"></script>
<script src="../reorder.v1.js"></script>

<h1>Dijkstra Test</h1>

<button type="button" onclick="dijkstraTest()">test</button>

<script>
    function dijkstraTest() {
	var nodes = [{id: 0}, {id: 1}, {id: 2}],
	links = [{source: 0, target: 1}, {source:1, target: 2}];
	var graph = reorder.graph(nodes, links)
	    .init();
	var dijkstra = reorder.dijkstra(graph).shortestPath(0, 2);
	console.log("Dijkstra: ", dijkstra);
    }

</script>
</html>
