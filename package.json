{
   "name": "reorder.js",
   "version": "2.0.0",
   "description": "Matrix reordering in JavaScript.",
   "keywords": [
      "es6",
      "visualization",
      "reordering",
      "matrix",
      "adjacency matrix",
      "parallel coordinates"
   ],
   "homepage": "https://github.com/jdfekete/reorder.js",
   "repository": {
      "type": "git",
      "url": "http://github.com/jdfekete/reorder.js.git"
   },
   "unpkg": "dist/reorder.min.js",
   "jsdelivr": "dist/reorder.min.js",
   "main": "dist/reorder.cjs.js",
   "module": "dist/reorder.esm.js",
   "browser": "dist/reorder.min.js",
   "types": "./reorder.d.ts",
   "author": [
      {
         "name": "Jean-Daniel Fekete",
         "url": "http://www.aviz.fr/~fekete"
      }
   ],
   "scripts": {
      "build": "rollup -c",
      "dev": "rollup -c rollup.config.dev.js -w",
      "pretest": "npm run build",
      "test": "vows --nocolor; echo",
      "clean": "rm -rf node_modules",
      "prettier": "prettier --write '{src,test}/**/*.js'",
      "docs": "jsdoc -c jsdoc.config.json",
      "check": "jshint src/*.js"
   },
   "files": [
      "dist"
   ],
   "dependencies": {
      "@sgratzl/science": "^2.0.0"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "^17.0.0",
      "@rollup/plugin-node-resolve": "^11.0.1",
      "jsdoc": "^3.6.6",
      "jshint": "^2.9.7",
      "jsonfile": "2.0.0",
      "parcoord-es": "^2.2.10",
      "prettier": "^2.2.1",
      "rollup": "^2.35.1",
      "rollup-plugin-livereload": "^2.0.0",
      "rollup-plugin-local-resolve": "^1.0.7",
      "rollup-plugin-serve": "^0.4.2",
      "rollup-plugin-terser": "^7.0.2",
      "rollup-plugin-version-injector": "^1.3.0",
      "seedrandom": "2.3.11",
      "vows": "^0.8.2"
   },
   "license": "BSD-2-Clause",
   "jshintConfig": {
      "esversion": 6,
       "varstmt": true,
       "undef": true,
       "unused": true,
       "globals": {
           "console": false
       }
   },
   "prettier": {
       "singleQuote": true
   }
}
